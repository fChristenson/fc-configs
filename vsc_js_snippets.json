{
    "Require": {
        "prefix": "qrequire",
        "body": [
            "var $1 = require('$2');"
        ],
        "description": "CommonJS require"
    },
    "include": {
        "prefix": "qinclude",
        "body": [
            "expect($1).to.include($2);"
        ],
        "description": "Chai include"
    },
    "Named function": {
        "prefix": "qnfn",
        "body": [
            "function $1($2) {",
            "  $3",
            "}"
        ],
        "description": "Named function"
    },
    "Anonymous function": {
        "prefix": "qfn",
        "body": [
            "function($1) {",
            "  $2",
            "}"
        ],
        "description": "Anonymous function"
    },
    "Log to console": {
        "prefix": "qlog",
        "body": [
            "console.log('$1');",
            "console.log('--------------------------');"
        ],
        "description": "Console.log"
    },
    "Module exports": {
        "prefix": "qme",
        "body": [
            "module.exports"
        ],
        "description": "module.exports"
    },
    "util": {
        "prefix": "qutil",
        "body": [
            "var util = require('util');"
        ],
        "description": "Node Util"
    },
    "fs": {
        "prefix": "qfs",
        "body": [
            "var fs = require('fs');"
        ],
        "description": "Node Fs"
    },
    "path": {
        "prefix": "qpath",
        "body": [
            "var path = require('path');"
        ],
        "description": "Node Path"
    },
    "inspect": {
        "prefix": "qinspect",
        "body": [
            "console.log(util.inspect($1, false, Infinity));",
            "console.log('-----------------------------------------');"
        ],
        "description": "Node Util Inspect"
    },
    "Describe": {
        "prefix": "qdescribe",
        "body": [
            "describe('$1', function() {",
            "  $2",
            "});"
        ],
        "description": "Mocha Describe"
    },
    "import": {
        "prefix": "qimport",
        "body": "import $1 from '$2';",
        "description": "ES6 import"
    },
    "it": {
        "prefix": "qit",
        "body": [
            "it('$1', function() {",
            "  $2",
            "});"
        ],
        "description": "Mocha it"
    },
    "fail": {
        "prefix": "qfail",
        "body": [
            "assert.fail(null, null, '$1');"
        ],
        "description": "Chai assert.fail"
    },
    "assert": {
        "prefix": "qassert",
        "body": [
            "var assert = require('chai').assert;"
        ],
        "description": "Chai assert"
    },
    "expect": {
        "prefix": "qexpect",
        "body": [
            "var expect = require('chai').expect;"
        ],
        "description": "Chai expect"
    },
    "equal": {
        "prefix": "qequal",
        "body": [
            "expect($1).to.equal($2);"
        ],
        "description": "Chai equal"
    },
    "after": {
        "prefix": "qafter",
        "body": [
            "after(function () {",
            "  $1",
            "});"
        ],
        "description": "Mocha after"
    },
    "before": {
        "prefix": "qbefore",
        "body": [
            "before(function () {",
            "  $1",
            "});"
        ],
        "description": "Mocha before"
    },
    "before_each": {
        "prefix": "qbeforeeach",
        "body": [
            "beforeEach(function () {",
            "  $1",
            "});"
        ],
        "description": "Mocha before each"
    },
    "ok": {
        "prefix": "qok",
        "body": [
            "expect($1).to.be.ok;"
        ],
        "description": "Chai ok"
    },
    "mapInspect": {
        "prefix": "qmapinspect",
        "body": [
            ".map(function(e) {",
            "  console.log(util.inspect(e, false, Infinity));",
            "  console.log('------------------------------------');",
            "return e;",
            "})"
        ],
        "description": "Inspect in map func"
    }
}
