{
    "Require": {
        "prefix": "qrequire",
        "body": [
            "const $1 = require(\"$2\");"
        ],
        "description": "CommonJS require"
    },
    "Named function": {
        "prefix": "qnfn",
        "body": [
            "const $1 = ($2) => {",
            "  $3",
            "};"
        ],
        "description": "Named function"
    },
    "Anonymous function": {
        "prefix": "qfn",
        "body": [
            "($1) => $2"
        ],
        "description": "Anonymous function"
    },
    "Log to console": {
        "prefix": "qlog",
        "body": [
            "console.log($1);",
            "console.log(\"--------------------------\");"
        ],
        "description": "Console.log"
    },
    "Module exports": {
        "prefix": "qme",
        "body": [
            "module.exports"
        ],
        "description": "module.exports"
    },
    "util": {
        "prefix": "qutil",
        "body": [
            "const util = require(\"util\");"
        ],
        "description": "Node Util"
    },
    "fs": {
        "prefix": "qfs",
        "body": [
            "const fs = require(\"fs\");"
        ],
        "description": "Node Fs"
    },
    "path": {
        "prefix": "qpath",
        "body": [
            "const path = require(\"path\");"
        ],
        "description": "Node Path"
    },
    "Describe": {
        "prefix": "qdescribe",
        "body": [
            "describe(\"$1\", () => {",
            "  $2",
            "});"
        ],
        "description": "Jest Describe"
    },
    "import": {
        "prefix": "qimport",
        "body": "import $1 from \"$2\";",
        "description": "ES6 import"
    },
    "it": {
        "prefix": "qit",
        "body": [
            "it(\"$1\", () => {",
            "  $2",
            "});"
        ],
        "description": "Jest it"
    },
    "fail": {
        "prefix": "qfail",
        "body": [
            "throw new Error(\"Fail\");"
        ],
        "description": "Fail unit test"
    },
    "equal": {
        "prefix": "qequal",
        "body": [
            "const expected = $1;",
            "const actual = $2;",
            "expect(expected).toEqual(actual);"
        ],
        "description": "Chai equal"
    },
    "after": {
        "prefix": "qafter",
        "body": [
            "afterAll(() => {",
            "  $1",
            "});"
        ],
        "description": "Jest after"
    },
    "before": {
        "prefix": "qbefore",
        "body": [
            "beforeAll(() => {",
            "  $1",
            "});"
        ],
        "description": "Jest before"
    },
    "before_each": {
        "prefix": "qbeforeeach",
        "body": [
            "beforeEach(() => {",
            "  $1",
            "});"
        ],
        "description": "Jest before each"
    },
    "after_each": {
        "prefix": "qaftereach",
        "body": [
            "afterEach(() => {",
            "  $1",
            "});"
        ],
        "description": "Jest before each"
    },
    "ok": {
        "prefix": "qdefined",
        "body": [
            "expect($1).toBeDefined();"
        ],
        "description": "Jest defined"
    },
    "express-server": {
        "prefix": "qserver",
        "body": [
            "const app = require(\"./src/app\");",
            "",
            "const port = process.env.PORT || 3000;",
            "",
            "app.listen(port, () => {",
            "  console.log(\"Running server on port:\", port);",
            "  console.log(\"--------------------------\");",
            "});",
            ""
        ],
        "description": "Express server"
    },
    "express-app": {
        "prefix": "qapp",
        "body": [
            "const express = require(\"express\");",
            "const app = express();",
            "",
            "app.use(express.json());",
            "",
            "app.get(\"/\", (req, res) => {",
            "  res.end(\"foo\");",
            "});",
            "",
            "module.exports = app;",
            ""
        ],
        "description": "Express app"
    },
    "express-static-content": {
        "prefix": "qstatic",
        "body": [
            "app.use(express.static(path.resolve(__dirname, \"..\", \"dist\")));"
        ],
        "description": "Express static middleware"
    },
    "webpack-config": {
        "prefix": "qwebpack",
        "body": [
            "const path = require(\"path\");",
            "",
            "module.exports = {",
            "  entry: path.resolve(__dirname, \"src\", \"public\", \"js\", \"main.js\"),",
            "  output: {",
            "    path: path.resolve(__dirname, \"dist\"),",
            "    filename: \"bundle.js\"",
            "  },",
            "  mode: \"development\"",
            "};",
            ""
        ],
        "description": "Express app"
    },
    "postcss-config": {
        "prefix": "qpostcss",
        "body": [
            "const postcssImport = require(\"postcss-import\");",
            "",
            "module.exports = {",
            "  plugins: [postcssImport]",
            "};",
            ""
        ],
        "description": "Express static middleware"
    },
}
