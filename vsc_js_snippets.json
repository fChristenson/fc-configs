{
    "Require": {
        "prefix": "qrequire",
        "body": [
            "const $1 = require('$2');"
        ],
        "description": "CommonJS require"
    },
    "include": {
        "prefix": "qinclude",
        "body": [
            "expect($1).to.include($2);"
        ],
        "description": "Chai include"
    },
    "Named function": {
        "prefix": "qnfn",
        "body": [
            "const $1 = ($2) => {",
            "  $3",
            "};"
        ],
        "description": "Named function"
    },
    "Anonymous function": {
        "prefix": "qfn",
        "body": [
            "($1) => $2"
        ],
        "description": "Anonymous function"
    },
    "Log to console": {
        "prefix": "qlog",
        "body": [
            "console.log($1);",
            "console.log('--------------------------');"
        ],
        "description": "Console.log"
    },
    "Module exports": {
        "prefix": "qme",
        "body": [
            "module.exports"
        ],
        "description": "module.exports"
    },
    "util": {
        "prefix": "qutil",
        "body": [
            "const util = require('util');"
        ],
        "description": "Node Util"
    },
    "fs": {
        "prefix": "qfs",
        "body": [
            "const fs = require('fs');"
        ],
        "description": "Node Fs"
    },
    "path": {
        "prefix": "qpath",
        "body": [
            "const path = require('path');"
        ],
        "description": "Node Path"
    },
    "Describe": {
        "prefix": "qdescribe",
        "body": [
            "describe('$1', () => {",
            "  $2",
            "});"
        ],
        "description": "Mocha Describe"
    },
    "import": {
        "prefix": "qimport",
        "body": "import $1 from '$2';",
        "description": "ES6 import"
    },
    "it": {
        "prefix": "qit",
        "body": [
            "it('$1', () => {",
            "  $2",
            "});"
        ],
        "description": "Mocha it"
    },
    "fail": {
        "prefix": "qfail",
        "body": [
            "throw new Error('Test not made');"
        ],
        "description": "Fail unit test"
    },
    "expect": {
        "prefix": "qexpect",
        "body": [
            "const expect = require('chai').expect;"
        ],
        "description": "Chai expect"
    },
    "sinon": {
        "prefix": "qsinon",
        "body": [
            "const sinon = require('sinon');"
        ],
        "description": "Chai expect"
    },
    "equal": {
        "prefix": "qequal",
        "body": [
            "const expected = $1;",
            "expect($2).to.equal(expected);"
        ],
        "description": "Chai equal"
    },
    "after": {
        "prefix": "qafter",
        "body": [
            "after(() => {",
            "  $1",
            "});"
        ],
        "description": "Mocha after"
    },
    "before": {
        "prefix": "qbefore",
        "body": [
            "before(() => {",
            "  $1",
            "});"
        ],
        "description": "Mocha before"
    },
    "before_each": {
        "prefix": "qbeforeeach",
        "body": [
            "beforeEach(() => {",
            "  $1",
            "});"
        ],
        "description": "Mocha before each"
    },
    "ok": {
        "prefix": "qok",
        "body": [
            "expect($1).to.be.ok;"
        ],
        "description": "Chai ok"
    }
}
